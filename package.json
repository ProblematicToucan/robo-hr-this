{
  "name": "cv-rag",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "typeorm": "typeorm-ts-node-commonjs -d src/db/data-source.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d src/db/data-source.ts migration:generate",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs -d src/db/data-source.ts migration:run",
    "migration:show": "typeorm-ts-node-commonjs -d src/db/data-source.ts migration:show",
    "migration:revert": "typeorm-ts-node-commonjs -d src/db/data-source.ts migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@qdrant/js-client-rest": "^1.7.0",
    "bullmq": "^5.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.1.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "pino": "^9.13.0",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0",
    "@types/node": "^24.6.2",
    "@types/pdf-parse": "^1.1.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
